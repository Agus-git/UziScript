testButton10Led9Encoding
	| script encoder |
	encoder := UziEncoder new.
	script := Uzi script: #main
		ticking: true
		delay: 10
		bytecodes: [:uzi | uzi
			pushLit: 9;
			pushLit: 10;
			primGetValue;
			primSetValue;
			end].
	self assert: (encoder encode: script) = #[
		"Stepping and steptime (true, 10)"
		16r80 16r00 16r00 16r0A
		
		"Literals (9, 10)" 
		16r02 16r08 16r09 16r0A
		
		"Variables"
		16r00
		
		"Bytecodes"
		16r00 16r01 16r50 16r51 16rFF]